      "lists": [
        [
          {
            "text": "Phil Heath won his second consecutive IFBB Mr. Olympia title",
            "links": [
              {
                "text": null,
                "type": "internal",
                "page": "Phil Heath"
              }
            ]
          },
          {
            "text": "Kai Greene placed second, his best ever showing after finishing third in the 2011 Mr. Olympia contest",
            "links": [
              {
                "text": null,
                "type": "internal",
                "page": "Kai Greene"
              },
              {
                "text": "2011 Mr. Olympia",
                "type": "internal",
                "page": "2011 Mr. Olympia"
              }
            ]
          },
          {
            "text": "Evan Centopani placed eighth in his Mr. Olympia debut",
            "links": [
              {
                "text": null,
                "type": "internal",
                "page": "Evan Centopani"
              }
            ]
          },
          {
            "text": "Toney Freeman, at the age of 46 years, finished in seventh place",
            "links": [
              {
                "text": null,
                "type": "internal",
                "page": "Toney Freeman"
              }
            ]
          },
          {
            "text": "Jay Cutler, four-time Mr. Olympia, could not compete after undergoing surgery for a torn left bicep",
            "links": [
              {
                "text": "Jay Cutler",
                "type": "internal",
                "page": "Jay Cutler (bodybuilder)"
              }
            ]
          }
        ]
