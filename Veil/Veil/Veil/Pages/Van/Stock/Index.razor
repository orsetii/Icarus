@page "/van/stock"
@using Veil.Data.Stock

<MudDataGrid Items="@StockItems">
    <Columns>
        <Column T="StockItem" Field="Name" />
    </Columns>
</MudDataGrid>

@code {
    private IEnumerable<StockItem> StockItems { get; set; }

    private StockService StockService { get; set; }

    public Index()
    {
        StockService = new StockService(); 
    }

    protected override async Task OnInitializedAsync()
    {
        StockItems = StockService.GetStockItems();
    }
}

